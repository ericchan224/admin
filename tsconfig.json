{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "global.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "components.d.ts",
    "auto-imports.d.ts",
    "typings/**/*.d.ts"
  ],
  "compilerOptions": {
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "ignoreDeprecations": "5.0"
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
